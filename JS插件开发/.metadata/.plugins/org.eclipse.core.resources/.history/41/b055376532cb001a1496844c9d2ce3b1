GeoBOS.filesystem = {
	
	init : function (call_back) {
		GeoBOS.plugin.execute("FileSystemPlugin", "init", GeoBOS.getId(), '', null, call_back);
	},
	openFolderOrFileByName : function (name, call_back) {
		GeoBOS.plugin.execute("FileSystemPlugin", "openFolderOrFileByName", GeoBOS.getId(), '', name, call_back);
	},
	returnPreFolder : function (call_back) {
		GeoBOS.plugin.execute("FileSystemPlugin", "returnPreFolder", GeoBOS.getId(), '', null, cb);
	}
}