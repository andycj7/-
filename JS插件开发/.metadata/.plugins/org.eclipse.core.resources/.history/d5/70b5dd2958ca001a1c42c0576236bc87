<body>
	<div class="title">Blank</div>
	<div class="line"></div>
	<div class="div-parent">
		<div class="div-child-1">
			<label class="label">当前路径</label> <input class="input-path" id="input_path"></input>
			<button class="button" onclick="init()">前往</button>
			<button class="button">返回</button>
		</div>
		<div id="div_child_2" class="div-child-2">
			<ul id="content"></ul>
		</div>
	</div>
</body>
<script src="js/GeoBOS.min.js"></script>
<script src="js/GeoBOS.filesystem.min.js"></script>
<script>
function init(){
	
	GeoBOS.filesystem.read(function(err, content){
		if (err) {
			document.getElementById("content").innerHTML = err.message;
		} else {
			document.getElementById("content").innerHTML = content;
		}
	});
}
</script>
